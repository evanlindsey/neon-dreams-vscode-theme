    {
      "name": "Markup - Plain (Angular Template Text)",
      "scope": [
        "text.html.markdown",
        "punctuation.definition.list_item.markdown"
      ],
      "settings": {
        "foreground": "#1A0F26" // Even darker purple template text, stark
      }
    },
    {
      "name": "Markup - Raw Inline",
      "scope": ["text.html.markdown markup.inline.raw.markdown"],
      "settings": {
        "foreground": "#01579B" // Darkest deep cyan inline, glowing
      }
    },
    {
      "name": "Markup - Raw Inline Punctuation",
      "scope": [
        "text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown"
      ],
      "settings": {
        "foreground": "#7E57C2" // Darker muted purple-gray punctuation
      }
    },
    {
      "name": "Markup - Heading",
      "scope": [
        "markdown.heading",
        "markup.heading | markup.heading entity.name",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#AD1457" // Darkest deep pink headings, pink power
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#007D34" // Darkest green italics, vivid
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold", "markup.bold string"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F57C00" // Darkest amber bold, warm pop
      }
    },
    {
      "name": "Markup - Bold-Italic",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold",
        "markup.quote markup.bold",
        "markup.bold markup.italic string",
        "markup.italic markup.bold string",
        "markup.quote markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#FF8F00" // Darkest brighter amber bold-italic, vivid
      }
    },
    {
      "name": "Markup - Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#D81B60" // Darkest brighter pink-red underline, wild contrast
      }
    },
    {
      "name": "Markup - Blockquote",
      "scope": ["markup.quote punctuation.definition.blockquote.markdown"],
      "settings": {
        "foreground": "#7E57C2" // Darker muted purple-gray blockquote
      }
    },
    {
      "name": "Markup - Quote",
      "scope": ["markup.quote"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#C2185B" // Darkest rich pink-red quotes, vibrant
      }
    },
    {
      "name": "Markup - Link",
      "scope": ["string.other.link.title.markdown"],
      "settings": {
        "foreground": "#01579B" // Darkest deep cyan links, glowing
      }
    },
    {
      "name": "Markup - Link Description",
      "scope": ["string.other.link.description.title.markdown"],
      "settings": {
        "foreground": "#F57C00" // Darkest amber descriptions, warm
      }
    },
    {
      "name": "Markup - Link Anchor",
      "scope": ["constant.other.reference.link.markdown"],
      "settings": {
        "foreground": "#0277BD" // Darkest brighter cyan anchors
      }
    },
    {
      "name": "Markup - Raw Block",
      "scope": ["markup.raw.block"],
      "settings": {
        "foreground": "#01579B" // Darkest deep cyan raw blocks, electric
      }
    },
    {
      "name": "Markup - Raw Block Fenced",
      "scope": ["markup.raw.block.fenced.markdown"],
      "settings": {
        "foreground": "#007D34" // Darkest green fences, vivid
      }
    },
    {
      "name": "Markup - Fenced Code Block",
      "scope": ["punctuation.definition.fenced.markdown"],
      "settings": {
        "foreground": "#C2185B" // Darkest rich pink-red fences, pink flair
